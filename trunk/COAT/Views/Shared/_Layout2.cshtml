<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>COAT: @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site2.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/templatemo_style.css")" rel="stylesheet" type="text/css" />
    <style type="text/css">
        .ui-progressbar .ui-progressbar-value { background-image: url(@Url.Content("~/Content/themes/base/images/pbar-ani.gif")); }
    </style>
    @RenderSection("Style",false)

    <script src="@Url.Content("~/Scripts/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.9.2.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-extend.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/coat-submitbutton.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/coat-common.js")" type="text/javascript"></script>
    @RenderSection("Head", false)
</head>
<body>
    <div class="templatemo_container">
        <div id="templatemo_header">
            <div id="templatemo_logo_area">
                <div id="templatemo_logo">
                    COAT</div>
                <div id="templatemo_slogan">
                    <img src="@Url.Content("~/Content/image/symLogo.png")" style="width:45%"/>
                </div>
                <div class="cleaner">
                </div>
            </div>
            <div id="templatemo_search">
                <div style="text-align: right; width: 100%; padding: 3px;">
                    @Html.Partial("_LogOnPartial")
                </div>
            </div>
            <div id="templatemo_menu">
                <ul>
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("ORP Validation", "Index", "Deals")</li>
                    <li>@Html.ActionLink("Other Management", "Index", "Contract")</li>
                    <li>@Html.ActionLink("Admin Management", "Index", "Admin")</li>
                </ul>
            </div>
        </div>
        <!-- end of header -->
        @RenderBody()
    </div>
    <!-- End Of Container -->
    <div id="templatemo_bottom_area">
        <div class="templatemo_container">
            <div class="cleaner">
            </div>
            <div id="templatemo_footer">
                PROPRIETARY/CONFIDENTIAL - Internal Use Only. Copyright 2012 Symantec Corporation.All
                rights reserved.
            </div>
        </div>
    </div>
    @RenderSection("Script", false)
</body>
<script type="text/javascript">
    $(document).ready(function () {
        $(".datepicker").datepicker({
            changeMonth: true,
            changeYear: true
        });

    });
</script>
</html>
